    "import os\n",
    "some  = []\n",
    "files = list(os.listdir())\n",
    "for i in files:\n",
    "    i = i.split('.')\n",
    "    i = i[0]\n",
    "    try:\n",
    "        i = i[-4] + i[-3] + i[-2] + i[-1]\n",
    "        if i == \"2004\":\n",
    "            some.append(i)\n",
    "    except:\n",
    "        continue\n",
    "print(some)"
